{
  "name": "jquery.extras",
  "version": "0.0.1",
  "description": "A library that extends jquery prototype with extra methods",
  "main": "dist/extras.js",
  "jsnext:main": "dist/extras.es.js",
  "scripts": {
    "dist": "rollup -c",
    "watch": "run-when-changed --watch \"dist/extras.js\" --watch \"test/test.js\" --exec \"npm test\"",
    "dev": "rollup -c -w",
    "test": "mocha ./test/*.js",
    "features": "run-when-changed --watch \"test/feature.js\" --exec \"node ./test/feature\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aptivator/jquery.extras.git"
  },
  "keywords": [
    "jquery",
    "extras",
    "helpers"
  ],
  "author": "Dmitriy A. Nesterkin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aptivator/jquery.extras/issues"
  },
  "homepage": "https://github.com/aptivator/jquery.extras#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "jsdom": "^9.12.0",
    "mocha": "^3.2.0",
    "rollup": "^0.41.6",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-watch": "^3.2.2",
    "run-when-changed": "^2.0.3"
  },
  "dependencies": {
    "jquery": "^3.2.1"
  }
}
